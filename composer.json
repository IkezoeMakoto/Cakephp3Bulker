{
    "name": "ikezoe-makoto/cakephp3bulker",
    "description": "cakephp3bulker plugin for CakePHP",
    "type": "cakephp-plugin",
    "license": "MIT",
    "require": {
        "cakephp/cakephp": "^3.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7|^6.0"
    },
    "autoload": {
        "psr-4": {
            "Cakephp3Bulker\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Cakephp3Bulker\\Test\\": "tests",
            "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
        }
    }
}
